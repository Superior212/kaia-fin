{
  "name": "kaia-finance-backend",
  "version": "1.0.0",
  "description": "Backend API for Kaia Finance with AI integration",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=2048' ts-node src/index.ts",
    "vercel-build": "tsc && echo 'Build completed'",
    "render-build": "tsc",
    "render-start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "postinstall": "npm run build",
    "test": "nodemon src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "kaia",
    "finance",
    "ai",
    "blockchain",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "Kaia Finance Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "mongoose": "^8.10.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.8.1",
    "uuid": "^10.0.0",
    "viem": "^2.19.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
